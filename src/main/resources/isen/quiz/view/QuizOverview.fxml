<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.text.TextFlow?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="isen.quiz.view.QuizOverviewController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <TextFlow fx:id="questionNumber" prefHeight="21.0" prefWidth="58.0">
      <opaqueInsets>
         <Insets />
      </opaqueInsets>
   </TextFlow>
   <Text fx:id="questionText" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
   <Button fx:id="answerButton1" mnemonicParsing="false" onAction="#handleAnswerButton1" text="Button1" />
   <Button fx:id="answerButton2" mnemonicParsing="false" onAction="#handleAnswerButton2" text="Button2" />
   <Button fx:id="answerButton3" mnemonicParsing="false" onAction="#handleAnswerButton3" text="Button3" />
   <Text fx:id="scoreText" strokeType="OUTSIDE" strokeWidth="0.0" text="score : 0">
      <VBox.margin>
         <Insets />
      </VBox.margin>
   </Text>

</VBox>
